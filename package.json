{
  "name": "lojban-parser",
  "version": "1.0.0",
  "description": "Lojban PEG parser with visualization",
  "type": "module",
  "scripts": {
    "start": "parcel src/index.html --no-cache --open",
    "build-core": "parcel build src/index.html --public-url ./ --no-source-maps && cp -avr *.md ./dist/ && yarn build:sw",
    "build:sw": "node ./tools/sw-gen.js",
    "seo": "npx --yes @divriots/jampack ./dist",
    "build": "yarn fetch-parser && yarn fetch-xraste && yarn build-core",
    "zip": "zip -r dist/artifact.zip ./dist",
    "serve": "cd dist && concurrently --kill-others --raw \"python3 -m http.server 8001\" \"ngrok http 8001\"",
    "fetch-parser": "rm -f src/camxes.js && wget -P src https://la-lojban.github.io/sutysisku-lojban-corpus-downloader/data/grammars/camxes.js",
    "fetch-xraste": "rm -f assets/parsed-xraste.json && wget -P assets https://la-lojban.github.io/sutysisku-lojban-corpus-downloader/data/parsed/parsed-xraste.json"
  },
  "resolutions": {
    "elkjs": "0.9.2",
    "@tweenjs/tween.js": "23.1.1"
  },
  "cache": {
    "disablePlugin": false,
    "inDev": false,
    "strategy": "inject",
    "clearDist": true
  },
  "dependencies": {
    "3d-force-graph": "1.73.2",
    "bootstrap": "^5.3.3",
    "comlink": "^4.4.1",
    "cytoscape": "3.28.1",
    "cytoscape-cise": "^1.0.0",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-context-menus": "4.1.0",
    "cytoscape-cose-bilkent": "4.1.0",
    "cytoscape-dagre": "2.5.0",
    "cytoscape-elk": "^2.2.0",
    "cytoscape-expand-collapse": "4.1.0",
    "cytoscape-klay": "^3.1.4",
    "cytoscape-navigator": "2.0.2",
    "cytoscape-node-html-label": "1.2.2",
    "d3": "^7.9.0",
    "d3-force-3d": "^3.0.5",
    "d3-octree": "^1.0.2",
    "elkjs": "0.9.2",
    "web-worker": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@divriots/jampack": "^0.23.3",
    "@parcel/transformer-sass": "2.12.0",
    "concurrently": "^8.2.2",
    "parcel": "2.12.0",
    "sharp": "^0.33.3",
    "workbox-build": "^7.0.0"
  },
  "keywords": [
    "javascript",
    "starter"
  ],
  "license": "MIT"
}
